&nbsp;
  values<span class="br0">&#91;</span>1<span class="br0">&#93;</span> <span class="sy0">=</span> <span class="nu16">5.4</span><span class="sy0">;</span>
  <a href="http://www.opengroup.org/onlinepubs/009695399/functions/cout.html"><span class="kw3">cout</span></a> <span class="sy0">&lt;&lt;</span> <span class="st0">&quot;ora values contiene: &quot;</span><span class="sy0">;</span>
