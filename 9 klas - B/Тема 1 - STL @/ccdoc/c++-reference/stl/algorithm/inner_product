    <span class="kw4">unsigned</span> m<span class="br0">&#91;</span>3<span class="br0">&#93;</span> <span class="sy0">=</span> <span class="br0">&#123;</span> 1<span class="sy0">&lt;&lt;</span>1<span class="sy0">,</span> 1<span class="sy0">&lt;&lt;</span>3<span class="sy0">,</span> 1<span class="sy0">&lt;&lt;</span>5 <span class="br0">&#125;</span><span class="sy0">;</span>
