/*
TASK:SCHOOL
LANG:C++
*/
#include<stdio.h>
int main()
{
int a[1000]={0};
int b[1000]={0};
int m=0;
int n=0;
int c[1000]={0};
int br=2;
int i=0;
int j=0;
int l=0;
int f=0;
scanf("%d %d",&n,&m);
for( ;i<m;)
{
scanf("%d %d",&a[i],&b[i]); i++;
}
br=0;
for(i=0; i<m; i++)
{l=0;
for(j=0; ;j++)
{if(a[i]==c[j]){l=2;break;}
else if(c[j]==0){l=1; break;}
}
if(l==1){c[f]=a[i]; c[f+1]=b[i]; br++; f++;}
else if(l==2){c[f]=b[i];}
f++;
}
printf("%d\n",br);
return 0;
}
