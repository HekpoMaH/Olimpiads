#include<iostream>
#include<string>
using namespace std;
string s2[]={"","ab","ba"};
string s3[]={"","abc","acb","bac","bca","cab","cba"};
string s4[]={"","abcd","abdc","acbd","acdb","adbc","adcb",
                "bacd","badc","bcad","bcda","bdac","bdca",
                "cabd","cadb","cbad","cbda","cdab","cdba",
                "dabc","dacd","dbac","dbca","dcab","dcba"};
int main()
{
  int n,k;
  scanf("%d %d", &n, &k);
  if(n==2)cout<<s2[k]<<endl;
  if(n==3)cout<<s3[k]<<endl;
  if(n==4)cout<<s4[k]<<endl;
}
