#include <iostream>
using namespace std;
int main()
{
    int n,l,p[1024],r[128],q[1024],f[128],i,mins,fs=0,sumn[128],m=INT_MAX;
    cin>>n>>l;
    for(i=1;i<=n;i++)
    {
        cin>>p[i];
        cin>>r[i];
        cin>>q[i];
        cin>>f[i];
        fs+=f[i];
    }
    if(fs<l) cout<<"-1"<<endl;
    return 0;
}
