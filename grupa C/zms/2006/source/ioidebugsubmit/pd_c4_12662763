for (int i=0;i<l;i++)
cin >> a[i];
if (n==1)
{cout << m;} else
if (n==3){
cin >> k1 >> m1 >> l1;
for (i=0;i<l;i++)
cin >> a1[i];
cin >> k2 >> m2 >> l2;
for (i=0;i<l;i++)
cin >> a2[i];
