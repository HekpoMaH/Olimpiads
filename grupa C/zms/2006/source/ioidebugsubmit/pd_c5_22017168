#include<iostream.h>
#include<string.h>
int i,br;
char n,s[256];
int main()
{
cin>>n;
//br=(strlen(n));
for(i=0;i<br;i++)
{
if(s[i]=='A') cout<<'.'<<'- ';
if(s[i]=='a') cout<<'.'<<'- ';
if(s[i]=='B') cout<<'-'<<'.'<<'.'<<'. ';
if(s[i]=='b') cout<<'-'<<'.'<<'.'<<'. ';
if(s[i]=='C') cout<<'-'<<'.'<<'-'<<'. ';
if(s[i]=='c') cout<<'-'<<'.'<<'-'<<'. ';
if(s[i]=='D') cout<<'-'<<'.'<<'. ';
if(s[i]=='d') cout<<'-'<<'.'<<'. ';
if(s[i]=='E') cout<<'. ';
if(s[i]=='e') cout<<'. ';
if(s[i]=='F') cout<<'.'<<'.'<<'-'<<'. ';
if(s[i]=='f') cout<<'.'<<'.'<<'-'<<'. ';
if(s[i]=='G') cout<<'-'<<'-'<<'. ';
if(s[i]=='g') cout<<'-'<<'-'<<'. ';
if(s[i]=='H') cout<<'.'<<'.'<<'.'<<'. ';
if(s[i]=='h') cout<<'.'<<'.'<<'.'<<'. ';
if(s[i]=='I') cout<<'.'<<'. ';
if(s[i]=='i') cout<<'.'<<'. ';
if(s[i]=='J') cout<<'.'<<'-'<<'-'<<'- ';
if(s[i]=='j') cout<<'.'<<'-'<<'-'<<'- ';
if(s[i]=='K') cout<<'-'<<'.'<<'- ';
if(s[i]=='k') cout<<'-'<<'.'<<'- ';
if(s[i]=='L') cout<<'.'<<'-'<<'.'<<'. ';
if(s[i]=='l') cout<<'.'<<'-'<<'.'<<'. ';
if(s[i]=='M') cout<<'-'<<'- ';
if(s[i]=='m') cout<<'-'<<'- ';
if(s[i]=='N') cout<<'-'<<'. ';
if(s[i]=='n') cout<<'-'<<'. ';
if(s[i]=='O') cout<<'-'<<'-'<<'- ';
if(s[i]=='o') cout<<'-'<<'-'<<'- ';
if(s[i]=='P') cout<<'.'<<'-'<<'-'<<'. ';
if(s[i]=='p') cout<<'.'<<'-'<<'-'<<'. ';
if(s[i]=='Q') cout<<'-'<<'-'<<'.'<<'- ';
if(s[i]=='q') cout<<'-'<<'-'<<'.'<<'- ';
if(s[i]=='R') cout<<'.'<<'-'<<'. ';
if(s[i]=='r') cout<<'.'<<'-'<<'. ';
if(s[i]=='S') cout<<'.'<<'.'<<'. ';
if(s[i]=='s') cout<<'.'<<'.'<<'. ';
if(s[i]=='T') cout<<'- ';
if(s[i]=='t') cout<<'- ';
if(s[i]=='U') cout<<'.'<<'.'<<'- ';
if(s[i]=='u') cout<<'.'<<'.'<<'- ';
if(s[i]=='V') cout<<'.'<<'.'<<'.'<<'- ';
if(s[i]=='v') cout<<'.'<<'.'<<'.'<<'- ';
if(s[i]=='W') cout<<'.'<<'-'<<'- ';
if(s[i]=='w') cout<<'.'<<'-'<<'- ';
if(s[i]=='X') cout<<'-'<<'.'<<'.'<<'- ';
if(s[i]=='x') cout<<'-'<<'.'<<'.'<<'- ';
if(s[i]=='Y') cout<<'-'<<'.'<<'-'<<'-'<<'- ';
if(s[i]=='y') cout<<'-'<<'.'<<'-'<<'-'<<'- ';
if(s[i]=='Z') cout<<'-'<<'-'<<'.'<<'. ';
if(s[i]=='z') cout<<'-'<<'-'<<'.'<<'. ';
if(s[i]=='1') cout<<'.'<<'-'<<'-'<<'-'<<'- ';
if(s[i]=='2') cout<<'.'<<'.'<<'-'<<'-'<<'- ';
if(s[i]=='3') cout<<'.'<<'.'<<'.'<<'-'<<'- ';
if(s[i]=='4') cout<<'.'<<'.'<<'.'<<'.'<<'- ';
if(s[i]=='5') cout<<'.'<<'.'<<'.'<<'.'<<'. ';
if(s[i]=='6') cout<<'-'<<'.'<<'.'<<'.'<<'. ';
if(s[i]=='7') cout<<'-'<<'-'<<'.'<<'.'<<'. ';
if(s[i]=='8') cout<<'-'<<'-'<<'-'<<'.'<<'. ';
if(s[i]=='9') cout<<'-'<<'-'<<'-'<<'-'<<'. ';
if(s[i]=='0') cout<<'-'<<'-'<<'-'<<'-'<<'- ';
if(s[i]=='.') cout<<'.'<<'-'<<'.'<<'-'<<'.'<<'- ';
if(s[i]==',') cout<<'-'<<'-'<<'.'<<'.'<<'-'<<'- ';
if(s[i]=='-') cout<<'-'<<'.'<<'.'<<'-'<<'. ';
if(s[i]=='?') cout<<'.'<<'.'<<'-'<<'-'<<'.'<<'. ';
if(s[i]==' ') cout<<'.'<<'-'<<'.'<<'-'<<'. ';
}
cout<<'.'<<'.'<<'.'<<'-'<<'.'<<'-'<<endl;
return 0;
}