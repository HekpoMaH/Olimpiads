/*
TASK: MORSE
LANG: C
*/


#include <stdio.h>
#include <ctype.h>

int i;
char a[256] = {0}, c = 0;

char cs[27][6] = { 
     ".-",   // A
     "-...", // B
     "-.-.", // C
     "-..",  // D
     ".",    // E
     "..-.", // F
     "....", // H
     "..",   // I
     ".---", // J
     "-.-",  // K
     ".-..",   // L
     "--",   // M
     "-.",   // N
     "---",  // O
     ".--.",  // P
     "--.-", // Q
     ".-.",  // R
     "...",  // S
     "-",    // T
     "..-",  // U
     "...-", // V
     ".--",  // W
     "-..-", // X
     "-.--", // Y
     "--.."  // Z
};

char ds [10][6] = {
     "-----",
     ".----",
     "..---",
     "...--",
     "....-",
     ".....",
     "-",
     "--",
     "---",
     "----"
};

int main () {
    for (i = 0;  ; i ++) {
        scanf ("%c", &c);
        c = toupper(c);
        if (c >= 'A' && c <= 'Z') {
              printf (cs[c - 'B']);
        }
        printf (" ");
    }
    return 0;
}
