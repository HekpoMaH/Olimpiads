
#include <iostream>
using namespace std;
int main ()
{
    int br=0, a, b, n, j, i;
    cin >> n;
    for (i=1; i<=n; i++)
      {cin >> a;
       for (j=1; j<=n; j++)
        {
         cin >> b;
        }
      if (a != b)
        br++;
      }
       cout << br << endl;
    return 0;

}

